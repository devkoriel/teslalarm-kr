# .env.test

# Telegram related
TELEGRAM_BOT_TOKEN=your_dummy_telegram_bot_token
TELEGRAM_CHAT_ID=-1000000000000

# OpenAI API key
OPENAI_API_KEY=your_dummy_openai_api_key

# Scraping interval (in seconds, 600 = 10 minutes)
SCRAPE_INTERVAL=300

# PostgreSQL connection: host is service name 'db' from docker-compose
DATABASE_URL=postgresql://dummy_user:dummy_password@db:5432/test_database

# Redis connection: host is service name 'redis' from docker-compose
REDIS_URL=redis://redis:6379

LOG_LEVEL=debug
