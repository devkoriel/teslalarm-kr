# .env.test

# Telegram 관련
TELEGRAM_BOT_TOKEN=your_dummy_telegram_bot_token
TELEGRAM_CHAT_ID=-1000000000000

# OpenAI API 키
OPENAI_API_KEY=your_dummy_openai_api_key

# 스크래핑 주기 (초 단위, 600 = 10분)
SCRAPE_INTERVAL=300

# PostgreSQL 연결: 호스트는 docker-compose의 서비스 이름 'db'를 사용
DATABASE_URL=postgresql://dummy_user:dummy_password@db:5432/test_database

# Redis 연결: 호스트는 docker-compose의 서비스 이름 'redis'를 사용
REDIS_URL=redis://redis:6379

LOG_LEVEL=debug
